    <Type>Bonsai.ChampalimaudHardware.AcqSystem.MotionSensorData, Bonsai.ChampalimaudHardware, Version=0.3.0.0, Culture=neutral, PublicKeyToken=null</Type>
  </ExtensionTypes>
</WorkflowBuilder>